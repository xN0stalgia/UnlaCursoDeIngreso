Algoritmo VarLogicas
	Definir p,q,pyq Como Logico;
	
	Escribir "P es V/F: ";
	Leer p;
	Escribir "Q es V/F: ";
	Leer  q;
	
	//nop<- no p;//noq <- no q;
	pyq <- p y q =Verdadero;
	
	Escribir "NOp: ",No p;
	Escribir "NOq: ",No q;
	Escribir "pyq: ",pyq;
	Escribir "pOq: ",p o q=Verdadero;
	
FinAlgoritmo
